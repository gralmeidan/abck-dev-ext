{
  "name": "Abck Plugin",
  "description": "Shows abck info",
  "version": "1.0",
  "manifest_version": 3,
  "background": {
    "service_worker": "src/background.ts"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting",
    "webRequest",
    "cookies"
  ],
  "devtools_page": "src/devtools/index.html",
  "host_permissions": [
    "*://*/*",
    "file://*/*"
  ]
}