{
  "name": "Abck Plugin",
  "description": "Shows abck info",
  "version": "1.0",
  "manifest_version": 3,
  "action": {
    "default_popup": "src/popup/index.html"
  },
  "background": {
    "service_worker": "src/background.ts"
  },
  "permissions": [
    "storage",
    "activeTab",
    "scripting"
  ]
}